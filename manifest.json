{
  "manifest_version": 2,
  "name":"Twitch Garbage Filter",
  "version":"0.2",
  "content_scripts": [
    {
      "matches":["https://www.twitch.tv/directory/*"],
      "run_at": "document_idle",
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "trash.png",
    "default_popup": "popup.html",
    "default_title": "Twitch Garbage Filter"
   }
}
